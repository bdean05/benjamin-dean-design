court la liste numberList\n            // à chaque itération on vérifie la présence de searchText à l'itération i\n            // si on trouve on s'arrête\n            if (searchText == numberList[i]) {\n                alert(\"Your number is in the list\")\n                return // on s'arrête\n            }\n        }\n        alert(\"Your number is not on the list\")\n    }\n\n    const findAseven = () => {\n        let compteur = 0\n        for (let i = 0; i < numberList.length; i++) {\n            if (numberList[i] % 7 == 0) {\n                compteur++\n            }\n        }\n        alert(compteur)\n    }\n\n    return (\n        <div>\n            <div style={{ padding: \"100px\" }}>\n                <input value={searchText} onChange={e => setSearchText(e.target.value)} placeholder=\"Find a number\" />\n                <button onClick={() => findElement()}>Verify</button>\n                <button onClick={() => findAseven()}>Find out if it's a seven multiple</button>\n            </div>\n            {\n                numberList.map((someNumber, index) => (\n                    <div key={index}>\n                        <button onClick={() => multipleSeven(someNumber)}>Click ton find out number</button>\n                    </div>\n                ))\n            }\n        </div >\n    )\n}\n\nexport default Exercice\n","/Users/benjamindean/Desktop/sites/Perso/benjamin-dean-design/src/components/Menu/Menu.js",["82","83","84","85"],"/Users/benjamindean/Desktop/sites/Perso/benjamin-dean-design/src/components/Menu/Menu.styled.js",[],"/Users/benjamindean/Desktop/sites/Perso/benjamin-dean-design/src/App.js",["86","87","88","89"],"/Users/benjamindean/Desktop/sites/Perso/benjamin-dean-design/src/components/Gallery.js",["90"],"import React, { useState, useEffect } from 'react';\nimport axios from 'axios';\n\n\n\nconst Gallery = (props) => {\n\n    const [listPhotos, setListPhotos] = useState([])\n    const [text, setText] = useState(null);\n\n    const searchNewPhoto = (e) => {\n        setText(e.target.value)\n        let foundPhotos = listPhotos.filter(photo => photo.title.includes(e.target.value))\n        setListPhotos(foundPhotos)\n    }\n\n    // lorsque le composant a terminé le chargement (le render)\n    // useEffect lance via axios la reqêute pour récupérer les photos depuis l'API jsonplaceholder\n    useEffect(() => {\n        axios.get('https://jsonplaceholder.typicode.com/photos').then(res => {\n            setListPhotos(res.data.slice(0, 20))\n        })\n    }, [])\n\n\n    return (\n        <div>\n            <input\n                type=\"text\"\n                className=\"search-box\"\n                onChange={searchNewPhoto}\n                value={text}\n            />\n            {listPhotos.map((photo, index) => (\n                <div>\n                    <img key={index} src={photo.url} />\n                    <h4>{photo.title}</h4>\n                </div>\n            ))}\n\n\n        </div>\n    )\n}\n\n\nexport default Gallery","/Users/benjamindean/Desktop/sites/Perso/benjamin-dean-design/src/components/Panier.js",["91"],"import React, { useState, useEffect } from 'react';\n\nconst Panier = (props) => {\n\n    const [listProducts, setListProducts] = useState([{\n        name: \"Apple mouse\",\n        price: 50,\n        endurance: \"5 years\",\n        quantity: 1\n    }, {\n        name: \"Apple Keyboard\",\n        price: 30,\n        endurance: \"4 years\",\n        quantity: 1\n    }, {\n        name: \"Apple MacBook\",\n        price: 1400,\n        endurance: \"10 years\",\n        quantity: 1\n    }])\n\n    const incrementQuantity = (productIndex) => {\n        let newProducts = [...listProducts]\n        newProducts[productIndex].quantity++\n        setListProducts(newProducts)\n    }\n\n    const decrementQuantity = (productIndex) => {\n        let newProducts = [...listProducts]\n        if (newProducts[productIndex].quantity > 1) {\n            newProducts[productIndex].quantity-- // on accède au \"productIndex\"-ième élément de la liste newProducts et on décrémente sa quantité\n            setListProducts(newProducts)\n        }\n    }\n\n    const reinitialateNumberOne = () => {\n        let newProducts = [...listProducts]\n        for (let i = 0; i < newProducts.length; i++) {\n            newProducts[i].quantity = 1;\n        }\n        setListProducts(newProducts)\n    }\n\n    const emptyItemList = () => {\n        setListProducts([])\n    }\n\n    const removeLastItem = () => {\n        let newProducts = [...listProducts]\n        newProducts.pop()\n        setListProducts(newProducts)\n    }\n\n    const deleteProduct = (productIndex) => {\n        let newProducts = [...listProducts]\n        if (productIndex > -1) {\n            newProducts.splice(productIndex, 1);\n        }\n        setListProducts(newProducts)\n    }\n\n\n    const resetProducts = () => {\n        setListProducts([{\n            name: \"Apple mouse\",\n            price: 50,\n            endurance: \"5 years\",\n            quantity: 1\n        }, {\n            name: \"Apple Keyboard\",\n            price: 30,\n            endurance: \"4 years\",\n            quantity: 1\n        }, {\n            name: \"Apple MacBook\",\n            price: 1400,\n            endurance: \"10 years\",\n            quantity: 1\n        }])\n    }\n\n    return (\n\n        <div style={{ marginTop: \"100px\" }}>\n\n            <button onClick={() => reinitialateNumberOne()}>Reset to one</button>\n            <button onClick={() => emptyItemList()}>Empty list</button>\n            <button onClick={() => removeLastItem()}>Remove last</button>\n            <button onClick={() => resetProducts()}>Previous list</button>\n\n\n            {listProducts.map((product, index) => (\n\n                <div key={index} >\n                    <h4>Name: {product.name}</h4>\n                    <h4>Unit price: {product.price}€</h4>\n                    <h4>Endurance: {product.endurance}</h4>\n                    <h4>Quantity: {product.quantity}</h4>\n                    <h4>Total price: {product.price * product.quantity}€</h4>\n                    <button onClick={() => incrementQuantity(index)}>+</button>\n                    <button onClick={() => decrementQuantity(index)}>-</button>\n                    <button onClick={() => deleteProduct(index)}>Delete</button>\n                    <hr></hr>\n                </div>\n\n            )\n\n            )}\n        </div>\n\n    )\n}\n\nexport default Panier",{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","replacedBy":"95"},{"ruleId":"96","severity":1,"message":"97","line":22,"column":25,"nodeType":"98","endLine":22,"endColumn":44},{"ruleId":"99","severity":1,"message":"100","line":22,"column":25,"nodeType":"98","endLine":22,"endColumn":44},{"ruleId":"99","severity":1,"message":"101","line":31,"column":29,"nodeType":"98","endLine":31,"endColumn":77},{"ruleId":"99","severity":1,"message":"101","line":36,"column":29,"nodeType":"98","endLine":36,"endColumn":77},{"ruleId":"99","severity":1,"message":"101","line":41,"column":29,"nodeType":"98","endLine":41,"endColumn":77},{"ruleId":"99","severity":1,"message":"101","line":46,"column":29,"nodeType":"98","endLine":46,"endColumn":77},{"ruleId":"92","replacedBy":"102"},{"ruleId":"94","replacedBy":"103"},{"ruleId":"92","replacedBy":"104"},{"ruleId":"94","replacedBy":"105"},{"ruleId":"106","severity":1,"message":"107","line":23,"column":81,"nodeType":"108","endLine":23,"endColumn":96},{"ruleId":"106","severity":1,"message":"107","line":24,"column":58,"nodeType":"108","endLine":24,"endColumn":73},{"ruleId":"106","severity":1,"message":"107","line":25,"column":67,"nodeType":"108","endLine":25,"endColumn":82},{"ruleId":"109","severity":1,"message":"110","line":15,"column":28,"nodeType":"111","messageId":"112","endLine":15,"endColumn":30},{"ruleId":"109","severity":1,"message":"110","line":31,"column":28,"nodeType":"111","messageId":"112","endLine":31,"endColumn":30},